FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      dAverage;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 20e-6;

boundaryField
{
    inlet           
    {
        type            fixedValue;
        value           uniform 20e-6;
    }

    outlet          
    {
        type            zeroGradient;
    }

    wall       
    {
        type            zeroGradient;
    }
}

